<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Evaluation of Ontology Learning</display-name>
  <servlet>
    <servlet-name>LoadFile</servlet-name>
    <servlet-class>com.ontoeval.controller.LoadFile</servlet-class>
  </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.ontoeval.controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadOntology</servlet-name>
        <servlet-class>com.ontoeval.controller.LoadOntology</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadFeatures</servlet-name>
        <servlet-class>com.ontoeval.controller.LoadFeatures</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvaluatedTerms</servlet-name>
        <servlet-class>com.ontoeval.controller.EvaluatedTerms</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EvaluatedRelations</servlet-name>
        <servlet-class>com.ontoeval.controller.EvaluatedRelations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadAdmin</servlet-name>
        <servlet-class>com.ontoeval.controller.LoadAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeRelevance</servlet-name>
        <servlet-class>com.ontoeval.controller.ChangeRelevance</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadFile</servlet-name>
        <url-pattern>/LoadFile</url-pattern>
    </servlet-mapping>
      <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
      </servlet-mapping>
      <servlet-mapping>
        <servlet-name>LoadOntology</servlet-name>
        <url-pattern>/LoadOntology</url-pattern>
      </servlet-mapping>
      <servlet-mapping>
        <servlet-name>LoadFeatures</servlet-name>
        <url-pattern>/LoadFeatures</url-pattern>
      </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EvaluatedTerms</servlet-name>
        <url-pattern>/EvaluatedTerms</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EvaluatedRelations</servlet-name>
        <url-pattern>/EvaluatedRelations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadAdmin</servlet-name>
        <url-pattern>/LoadAdmin</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ChangeRelevance</servlet-name>
        <url-pattern>/ChangeRelevance</url-pattern>
    </servlet-mapping>

</web-app>
