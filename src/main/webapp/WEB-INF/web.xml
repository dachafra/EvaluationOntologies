<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Evaluation of Ontology Learning</display-name>
  <servlet>
    <servlet-name>LoadFile</servlet-name>
    <servlet-class>com.ontoeval.controller.LoadFile</servlet-class>
  </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.ontoeval.controller.Login</servlet-class>
    </servlet>
  <servlet-mapping>
    <servlet-name>LoadFile</servlet-name>
    <url-pattern>/LoadFile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>

</web-app>
